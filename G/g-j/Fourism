            $od = dd(d);
            od.path.pop();
            od.d--;
            out.push( me&ind,s.y.next,y,od );
        }

        !d.cbfirst and one = y(s,d)
        d.not and return
        out.unshift(one);

        out = out.filter(&s{ return 1 && s });
        !out.length and return
