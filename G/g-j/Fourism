                $l = [d.t,d.y.cv];
                each in ds&z {
                    l.push(more(n))
                }
                return l
            };
            localStorage[store] = enj(more(d));
        }
        else {
            $s = localStorage[store];
            !s and return L.sc[k] = Cye([k,1,{prefab:1}])
            $more = &l{
                $d = Cye([l.shift(),l.shift()]);
                each in l {
                    fio(d,more(n))
                }
                return d
            };
            d = L.sc[k] = more(dej(s))
            dc&thaw = 1
        }
        return d
    };
