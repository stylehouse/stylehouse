        # they persist
        $was = Rs&eCsc && Rs&eCsc[t];
        # keep the later wave of style
        was && weakly and return
        isst(c) and c = G&peel,c
        # realise work is done
        was && heq(c,was) and return &{}
        
        ah(R,'eCsc',t,c);
        # returns hash of css
        $ctocss = &c{
            $C = Cye([t+'-style',1]);
            ex(C.sc,c)
            $st = 4s&styler ||= me&styler;
            $css = {};
            st(C,css);
            return css
        };
        return &el,{
            $css = ctocss(c);
            was and was = ctocss(was)
            each kv was {
                css[k] and continue
                $(el).css(k,'');
            }
            was = null;
            $(el).css(css)
        };
    }
    # R's element
    me.Dispon = &acgtR{
        $r = me&Disp,R;
        return r && rs&on
    };
