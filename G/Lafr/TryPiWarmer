        $l = ['1', '2', '3',1]
        hat('indexOf string!=num',{l,
            for_num_1: l.indexOf(1),
            for_string_1: l.indexOf('1'),
            for_num_2: l.indexOf(2),
            for_string_2: l.indexOf('2'),
        })

