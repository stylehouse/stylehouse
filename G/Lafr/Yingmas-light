# usual display case, for gy&can o,i,c
me.case = &acgtt{
    isnu(t) and $cv = sca(t); t = null
    $g = me&opeolo,t||'Z';
    gy&cv = cv || 0.7;
    gs&dis = 5;
    gy&label.sc.dis = 0;
    # this is for some css subtlety
    $p = g;
    g = gy&can('w','aving');
    gy&notifempty = &{
        !(gs&z||[]).length and p.c.not = 1;
    };
    gy&up = p;
    return g;
};
