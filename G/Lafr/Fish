    fi.complace = &acgtsc{
        # $Ct-eight, $i++-seven
        $seven = fi&new_seven,c
        # put for &com into
        $done_Gstac = me&Gstaci,'-log',seven
        $done_comless = fi&comlesslogs,seven
        # and this first -log
        # < notices all Gc&logs/-seven active
        $D = fi&com,s,c
        #me&nu,"complace",{C,This,eight,seven,D}
        return &{
            done_comless()
            done_Gstac()
        }
    }
    # R:-eight/R:-seven created
