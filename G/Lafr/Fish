        return seven
    }
    # name -eight by &complace c.*, eg c.A -> "A:"+A.t
    fi.complaceCt = &acgtc{
        # < with context, realise $n to dup names:
        #   i This/Ct/$k/$v/$n:-eight
        $k = haks(c)[0]
        $v = c[k]
        !isC(v) and throw "!C"
        $Ct = k+':'+v.t
        return Ct
    }
