                    $q = {Goable:t.t}
                    hs&mot != mot and q.anyway = 1
                    # -> @Goting
                    me&nodei,R,Jxs&Goting,h,{},q
                    q.anyway and return
                    # visual
                    $row = hy&arow
                    $te = row.te
                    tes&bri = 1.6
                    $se = xy&stepcol('mot',row)
                    ses&bri = 1.6
                })
                # < parallel
                break
            }
            break
        }
        
    },'ift,D'],
  
  // 4 @Goting
    # %%Go-ing Travel/$t
    ['Goting',0.4,&acgtRs{
        $Jx = me&Jx,R
        $Tr = Jxs&Travel
        
