        me.sixing = &acgtRc{
            $Jx = me&Jx,R
            $D = c.D
            $se = c.se
            $sixing = me&opeolo,'-six',{se}
            if (sixings&ope) {
                $six = Dy&six
                # displays
                $ol = me&yology,six
                ac(R,'M', ol)
                oly&cv = 0.7
                me&nu,"D",D
                Jx.ro("see-string?",{}) &&
                n streng 7 $s:Ds&string %e:2,mh:30
            }
