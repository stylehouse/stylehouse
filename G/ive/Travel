    }
    # waits_8 noticing later[] etc woke this time
    me.Detectioned = &acgtRs{
        !Rs&Ying and throw "asYing"
        !isnode(s) and throw "!node"
        $D = Rs&Det
        $r = sy&R
        $k = ry&up == R ? "step" : "node"
        $c = {}
        c[k] = s
        return me&bracko,R,D,c
    }
    # receive what Ying wants to do
    me.Yingate_8 = &acgtRc{
        $D = delete Rs&Det
        Dc&be_time = Gtime()
        me&Dwakers,R,D,c
        me&Dtunneling,R,D
    }
  // &log -> %Outlog/$D
    # D -log
    me.log = &acgtRtqc{
        $D = me&Outlog,R,'log'
        Dc&s = t
        $sc = {}
        t and sc.says = t
        q and ex(sc,q)
        isnode(Rc&s) and sc.node ||= Rc&s
        hak(sc) and me&bracki,R,D,sc
        c and ex(D.c,c)
        return D
    }
    # accessor
