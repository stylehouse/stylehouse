      // back
        # for h that may be|produce hi%%historic
        #  c.shade/$k - delete hi%$k (only h %%lowel)
        #  c.x - (&columns) hide current %%historic row
        me.historicalise = &acgtRc{
            $hi = c.hi
            $lo = c.lo
            if (!hi) {
                # just $tid++
                hi = Cye(['historicalise',1])
                his&historic = 1
                ex(hi.sc,lo.sc)
                if (c.shade) {
                    haks(peel(c.shade))
                        .map(k => delete hi.sc[k])
                }
