                        !me&hall,thislimit,only and continue
                        me&Ri,j,{limit:t,sublated:r}
                    }
                }
            }
        }
        
        $C = Rs&C
        each ir Rc&N {
            if (rs&strange) {
                # < partly glowing map of t**
                $tpath = joint(me.cby(rs&E,E => Ec&top),'/')
                $tc = me&tc,'strange:'+tpath,'16b'
                
                delete tcy&cv
            }
        }
