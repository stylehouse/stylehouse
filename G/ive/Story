                $N = r.sc[k]
                !N and continue
                # calls &Rlimito, etc, in order
                me&mecy,['R',k,'o'],r,{N}
            }
        }
        # < generating weightiness
        each ir Rc&N {
            # R%M + R%Mw.* -> R%M, spaced
            me&modulatM,r
        }
        Rc&N.map(r => me&specialfx1,r)
        reverse(Rc&N).map(r => me&specialfx2,r)
    }
    # R%M += R%Mw/T.act.t|$dome/$modulat**
    me.modulatM = &acgtR{
