  // &Ri|o - magic-aware brack...
    # < brack at e, then give it to R when alive, or always on E?
    me.Rioing = &acgtr{
        $args = [...arguments].slice(4)
        isR(r) and args.shift()
        else r = 0
        R = r || me&R
        $c = {R,args}
        return c
    }
    # for &Ri,'limit:ref' -> &ha,... + R%limit=[]
