        }
        #me&nu,"Newsevens",{N,r}
    },'ift,D'],
    
    ['FishThis',0.293,&acgtRs{
        me&reaw,R,'FishThis,wakeful,onlyish'
        me&inode,R
        $x = me&Jx,R
        # wake R when p%onlyish/R%wakeful
        Rs&wakeful = 1
        # sleep r when R%onlyish/r!%wakeful
        Rs&onlyish = 1
