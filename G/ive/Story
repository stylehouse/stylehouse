        $r = me&Ret,R,c
        ry&cv = 0
        
        # main act, pre-&Ring
        me&Pieringi,R,r,c
        
        if (c.Top) {
            rc&N = []
            rc&d = 0
            rc&dl = 15
            rc&nl = 300
            delete rc&refR
            delete rc&refs
