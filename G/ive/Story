    # interface to have R/r that return r@295
    ['doRbales',0.294,&acgtRs{
        sy&balein = &sc{
            return fi&balein,s,c
        }
    },'ift,D'],
    ['doRbales',0.296,&acgtRs{
        fi&Rbales
    },'ift,D'],
    
    ['newsevens',0.7,&acgtRs{
