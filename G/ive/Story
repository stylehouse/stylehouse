    # match brack row with order
    me.hal = &acgtnc{
        c = peel(c)
        # check it has them
        each tv c {
            !hak(n.sc,t) and return 0
            v != 1 && n.sc[t] != v and return 0
        }
        # check order
        $ks = haks(c)
        each kv n.sc {
            !hak(c,k) and continue
            $be = ks.shift()
            be != k and return 0
        }
        return 1
    }
    # compose (concat l) name for and call me function
