    self.lt = &lt{
        t > 1 and t = '0.'+t
        return sz(l,t)
    }
    self.sz = &ns{
        isob(n) and throw "pass value||null"
        return (n || 0) < s
    }
    # match s=sc strictly against c={has:{limit:t},may:{toomuch:1}}
    me.honly = &acgtsc{
        each kv s {
            c.has && (c.has[k] == 1 || c.has[k] == v) and continue
            c.may && (c.may[k] == 1 || c.may[k] == v) and continue
            return 0
        }
        each kv c.has {
            !hak(s,k) and return 0
        }
        return 1
        
    }
    me.hall = &acgtNy{
        each in N {
            !y (n) and return 0
        }
        return 1
    }
    me.hany = &acgtNy{
