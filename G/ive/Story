            me&arriveRbitX,r,r.t
            # new J, normal mind
            rs&task ||= 'Pi_The'
        }
        # 13 puts on tape or not
        # < splice in re ry&up, neighbours
        rc&N.push(r)
        
        # to &Eing
        return r
    }
    
  // R'The' stylemo
    # 3 realises R/r as E/e
    
    # @ 22 layouty TXi
    me.TXin = &acgttg{
        g = peel(g)
        $R = me&R
        $E = Rs&E
        $sol = &he{
            each kc h {
                $name = joint([t,e&&e.t,k])
                $z = delete c.z
                $init = delete c.init
                # c becomes aq%qua
                $aq = fi&TXi,e||E,name,{t:k},c
                
                ex(aq.sc,g)
        # < most matches want to apply only to E/*
        #   applying further is wander, ie E/**
        #   then there's one-per:
        #    putting the RX.ignore on the matcher
                ah(aq,'also','dl',1)
                # < use
                init and init(aq)
                # then more inside
                z and sol(z,aq)
            }
        }
        return &h{ sol(h) }
    }
    
