=ffmpeg
 wasm https://fileconverter.digital/
 speed up audio:
  ffmpeg -i MVI_0661.MOV -filter:a "atempo=2.0,atempo=2.0,atempo=2.0,atempo=1.2" MVI_0661.mp3
 speed up video:
  find . -regex '.*MOV' -exec ffmpeg -i {} -b:v 8M -filter:v "setpts=0.1*PTS" -an {}.mp4 \;
  

