            me&anystrange and return Ds&fail = 1
            # from our nearest node
            $r = me.cbu(R,'Piing')
            each in rows {
                me&nodei,r,t,n.sc
            }
        },'ift,D'],
        ['pretendpi',0.327,&acgtRs{
            delete R.sc[Rs&pretendpi]
        },'ift,D'],
       // %Labo compress again
        ['Labo',0.22,&acgtRs{
            $E = Rs&E
            Rs&bell = 1
            Rs&climbD = 1
            $p = Ry&up
            if (ps&top_bell) {
                # due to Nine/-Lab (top of the outsphere)
                # wanting to include %tige,dige
