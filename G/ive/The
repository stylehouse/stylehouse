     // for &Super* times
        # vate:r%Piing <- %%Superdef for a vate
        me.Superdef = &acgtc{
            $R = me&R
            if (Rs&Piing || Rs&TheFine) {
                # outsphere
                # < organise &Super* via &Superpath
                $r = R
                c.wakenode ||= 1
            }
            else {
                # or in &Piing
                # eg -Lab/-Labi defining what it would push
