            }
            me&Eing,6
        },'ift,D'],
        ['Labi+nodei',0.326,&acgtRs{
            $E = Rs&E
            $D = Rs&D
            # destination
            $eto = me&fiu,E,'eto'
            !eto and me&strange,"!eto"
            $t = eto && etos&target
