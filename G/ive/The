    ['sleeping+Piing',0.296,&acgtRs{
        hak(sy&path) == 1 and Rs&eph = 1
        #~>4 Piing-sleeps: joint(sy&path)
    },'ift,D'],
    ['Piing',0.36,&acgtRs{
