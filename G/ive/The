                #  only %%limit:dl,toomuch?
                $only = n => me&honly,n.sc,{has:{limit:t},may:{toomuch:1}}
                each ij hN {
                    $thislimit = js&limit.filter(n => ns&limit == t)
                    !me&hall,thislimit,only and continue
                    me&Ri,j,{limit:t,sublated:r}
                }
            }
        }
    }
    # post-thingo, pre-measure
    me.Topping_37 = &acgtR{
