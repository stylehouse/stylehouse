            !xs&Fories and ha('pur:!Fories')
            # the bunch of Nines we have open
            $F = xs&TheF
            !F and ha('pur:!F')
            ha('arg',{F})
            # < have Thy first clue from F
            !xs&Thy and ha('pur:!Thy')
        me&Superready and return
            me&Superarg,{Gome:s}
          # get @Cat to control
            me&nodei,R,xs&Travel,Go,{},{Gome:s,Catfunc:'PiiCat'}
            # fires back
