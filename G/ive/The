
me.Sch_The = &acgt{ return [
 // The**
    
    =The
     little %TX -pi circuit
     user generating selections
      could mean any of the coordinates for it
      a multiple select should find language that expresses it
     use R
    =cut
    =neupi
     R'The' does -hat or -Bat
      makes $E** to $D**
       < from $D** as decoded
      < display entirely via $D
      encodes $D
     # used in reality to|from:
     Thyer/
      This...$got
       usu -hat etc as a data dumper
       in-time activity
      The...$exp
       usu -Bat pile for E%TX input
       are essentially expectations
        might be a negative assertion
        might modulate the encoding
     # experienced by:
     Gnome/
      Fories/ is storage for any:
      TheF/$t/$t%TheFine/$ti%TheFe tick over
       $ti goes OK
        has $exp=$ti-Ness/-eight/-seven/etc
        and $got=-Ness/... that &EsyncThis from This/*/*/etc
        the $exp%dige lets you avoid opening W:$ti until fail
      Thy/
       < %TX staged $ti <-> The...$exp
       < rename Theage?
        
      
     
     < breaking up view controllers into composed units
        data sources, navigators, interactions, etc
      
     $E
      seeds of $R
      $E/* become @3
      E%TX and E%RX
       rudimentary ref recycling (&Eref*)
     $D for recording
      Dc&pi changes as it types up:
      -val
       -hash
        -C
         -n locatable from a namable node
       etc
      a one-line instant of data-point observation
      in the $D-sphere
       is the storable -log/**
     
     E%RX
      simple paths in ($t**) can be made
      < everything else
     
     < $D @6 sleps?
       may be able to 'OK', no modulat default
     
     < $D @7 converts to modulat
       $D sometimes rehydrates
        without needing the data (Rc&s)
         $D-got
          may make local expansion of sc&s it has
           but want a new -log to confirm it in-time
         $D-exp
          cannot expand
     
     < $D @8 is the click-unit
       somethings in the modulat may further instruct:
     
     %TheeThy is a %Story
      /%TheThis does eg -nine
      /%TheThe Troom for /$Tt
     
     This shows stuff, potentially out of time
      -log will be Ring by Eight
     < The..#$D
       your clicks being configured
       selected one shows CED columns over slope rows
    
    =cut
  
  # ensures This is The
    &{ me.Thisisinside = 0 ? 'Thyer' : 'Piing' },
  // Gnomp - simplified @Gump...@Gome
    ['Gnomp',0.296,&acgtRs{
        $x = me&Jx,R
        me&reaw,R,'Gnomp'
        me&baleg,'TryPiWarmer','Gnome'
        me&quietready,R and return
        me&locked,s,'Gnome' and return
        me&tc,"free"
    },'ift,D'],
    ['Gnome',0.5,&acgtRs{
        # from &Gome_permit:
        me&reaw,R,'Gome,client'
        Rs&client = 1
        me&acting,R,'Gnome'
        $x = me&Jx,R
        $Gnomp = isnode(s)
        me&lock,Gnomp,'Gnome',s
        # in time %%quiet_wake:Gump
        Rs&wakeful = 1
        $quiet = me&is_quiet_now,Gnomp
        quiet and me&tsc,'quiet'
        quiet and Rs&Gump_quiet = 1
        me&tc,"\n"
        
        # &Gome_Jx shall:
        #  xs&Gome = s)%Gnome here, and xs&*=s/*
        #   s.t=TestName
        #  xs&Run = Piing, has /This etc
        
        !me&SuperGnome,R,s,{} and return
        me&unlock,Gnomp,'Gnome',s
    },'ift,D'],
    ['Gnomp',0.6,&acgtRs{
        # R%wakeful if (s/*)%wakeful
        me&ioty,s .map(&z{
            $r = zy&R
            !rs&wakeful and return
            Rs&wakeful = 1
            me&tc,"wake:"+r.t
        })
    },'ift,D,sleeping'],
    ['Gome',0.52,&acgtRs{
        me&jaa,R,'pre' .map(&n{
            ns&wakeful and '20s timer'
            ns&restless and '4s timer'
            # wakes us when ^^Gump%quiet_now
            ns&restless || ns&wakeful and Rs&wakeful = 1
        })
    },'ift,D'],
    # where non -Ness Fine/$t are staged for Piing/The...
    ['TheThy',0.2901,&acgtRs{
        me&reaw,R,'TheThy'
        me&acting,R
        !me.cbu(R,'Gnome') and return
        Rs&wakeful = 1
        Rs&Piing = 1
    },'ift,D'],
  // &SuperGnome the waking mind
    &{
        me.SuperGnome = &acgtRsce{
        # SuperGome for less moving parts
        # 1: init %Gome/*
         // 1
            $x = me&Jx,R
            c.t ||= 'Gnome'
            $su = me&supetime,R,c.t
            su('Reside',{perm:&zaa,jaa,ha,ar{
                x = me&Gome_Jx,R
                # not waiting on much to get ready
                !x and return ha('pur:t+SC')
                # we identify as this for Story funcs
                s != xs&Gome and debugger
                
                # < &Effect each $t in there, to create it?
                me&Gome_Resides,R,{t:s.t}
                
                # wait til quiet once
                # < meaning Gome/* are ready
                # want ever_quiet once here to continue
                me&Superwaits,R,{ha,zaa,t:'Resides',until_next:Rs&Gump_quiet}
                
                x = me&Gome_Jx,R
                !xs&Fories and ha('pur:!Fories')
            }})
            &&
        # 2:
        #  knowing how, %%Control -> $SC +/*
        #  until %%Releasor exits
        #   < should only have to pause until pushed:
        #    to store J osc via Duolish** &SuperGansp
         // 2 see things
            su('SC/*',{until:&zaa,jaa,ha,ar{
                # enable @Intro
                #me&nodei,R,xs&Introtivity,{Intro:1,Cat:SC}
                x.ro('init',{}) and me&nodei,R,xs&Piing,'initdata'
                
                x = me&Gome_Jx,R
                !x and debugger
                # < should stay
                $Run = xs&Run
                $F = xs&TheF
                !Run and return ha('pur:!Run')
                !F and return ha('pur:!F')
                
                # puts xs&This
                $This = me&Gome_This,R

                # we then expect to go quiet
                # allow Gump to quiet wake while ~This**
                sy&want_quiet_wake = &{
                    $is = me&Gome_Thischa_other,R,F
                    $say = is ? 'wakes' : ''
                    ~>8 GomeThischa: say
                    #is and debugger
                    return is
                }

                !This and return ha('pur:!This')

                me&Gome_Thischa_self,R,F,ha

                # observe Proto !now not over finishline

                Rs&goGome and me&nu,"Fins",{Run,This,F}
                
                x.ro('end') and return
                return ha('pur:notend')
            }})
            
            return su('done')
        }
    },
  
  // Pi%Lab: Nine/* > Thy > The,This
   
   // Story sends Nine/!-Ness for Thy/This/...
    &{
    # %TheFine has E-nine'd
    me.Fine_aft_nine = &acgtREsx{
        # e Thy/This
        $Thy = xs&Thy
        $Thys = Thy && me&fiu,Thy,'This'
        !Thys and return me&tc,'!Thys','b'
        $N = me&ioty,s
        each in N {
            if (nc&pi == 'Ness') {
                !(Es&Is && Es&Is.includes(n)) and break
                continue
            }
            # %%n%tige brackstills
            Thys and me&nodei,R,Thys,{inst:1,n}
        }
    }
    # %Piing 296 arrives This/
    me.Piing_receives_inst = &acgtRs{
        me&jaa,R,{inst:1,eto:s} .map(&n{
            $Fine = ns&efrom
            $t = ns&n
            if (Fine != sy&up) {
                # first hop, to Thy/This/* o Nine
                $z = me&Pii,Fine.t+'-Labine'
            }
            elsif (sc&s == t) {
                return
                # has arrived at:
            }
            else {
                # then Thy/This/Nine/$Outs
                # < lv to Outs=meddle
                $z = me&Pii,t.t+'-Lab'
                zc&s = t
            }
            !zy&R and return me&waits,'fwd:inst'
            me&nodei,R,z,n
        })
    }
   
   // Pi%Lab
    me.Pi_Lab = &acgt{ return [
        # loads Nine/-Lab, into -Bat preparator
        #  or, posit Nine/-Lab from -Bat
        #   will need to select all etivity about time...
        #   
        ['Lab',0.22,&acgtRs{
            $V = Rs&V
            $E = Rs&E
            $D = Rs&D
            me&tc,'Lab','g'
            # outsphere: loads Nine/s-Lab
            $e = me&Ei,{t:'Outs',pi:'Labe',s:s}
            Ey&OutsE = e
            me&Eing,6
            
            # insphere: -Bat preparator
            $e = me&Ei,{t:'Ins',pi:'Labi',s:ey&D}
            Ey&InsE = e
            me&Eing,6
            
            # outsphere: -Bat compressor
            $e = me&Ei,{t:'Ino',pi:'Labo',s:ey&D}
            Ey&InoE = e
            me&Eing,6
            
            if (V) {
                # let TheFine sense this state, like -Eight%dige
                #  its &piLab will use instead:
                $details = [Ey&InsE,Ey&InoE]
                Vs&dige = dig(details.map(e => ey&thing)
                    .map(t => ts&dige).join("")
                )
            }
