            }
        },'ift,D'],
        
        
        
        ['Labo',0.32,&acgtRs{
            $E = Rs&E
            $D = Rs&D
            Ec&noencode and return me&tc,'eph','6y'
            me&typeup_Dpi,R,s
            if (Rs&nodei) {
                $eto = me&fiu,E,'eto',8
                !eto and return me&strange,"!eto"
                $z = etoy&D
                # < trying to follow|update how %Labe did it
                zc&s == s.t and etoc&noencode = 1
            }
        },'ift,D'],
    ] }
    },
  
  // &Piing Thee
   // %Piing nodes
    ['Piing',0.291,&acgtRs{
        $x = me&Thee
    },'ift,D'],
    &{
    # o a node
    me.Thee = &acgt{
        $R = me&R
        # recur id
        $c = {}
        $x = me&Theing,c
        xy&c = c
        !c.long and throw "Thee!long"
        me&bin,c.long
        Rs&TheingE = 1
        Rs&onlyish = 1
        # in space
        xy&Thees = me.cby(R,'^^^'+c.long)
        return x
    }
    # identity spreader, more of the above
    me.Theing = &acgtc{
        $R = me&R
        $s = Rc&s
        $x = me&Jx,R
        Rs&Theingx && c and throw "const"
        Rs&Theingx and return Rs&Theingx
        Rs&Theingx = x
        c ||= {}
        # believing something
        # < R%tow going?
        ss&tow and Rs&tow = 1
        
        # receivable somewhere
        Rs&elvis = 'FishElvis'
        
        c.long ||= T.act.t
        c.long and me&reaw,R,c.long+',elvis,tow'
        c.short ||= c.long
        c.short and xy&go = me&acting,R,c.short
        c.in and me&bin,c.in
        
        return x
    }
    # i a node
    me.Pii = &acgtsc{
        $R = me&R
        isst(s) and c = s; s = Rc&s
        $v = me&Rsci,c,{v:s}
        return v
    }
    },
   
   // %Piing**
    ['Piing',0.296,&acgtRs{
        $x = me&Theing
        $E = Rs&E
        $p = Ry&up
        
        if (!Rs&eph) {
            # tv, %Piing** is tw
            # may panic when sy&up not reciprocal
