   // &Piing
    &{
    # ring node's E/*
    me.Piing = &acgtc{
        isst(c) and c = me&t_tpi,{t:c}
        $x = me&Theing
        x.ro('noR') and c.sleeping = 1
        $R = me&R
        $s = Rc&s
        $E = Rs&E
        
        # T from the place
        $tc = me&Thetower,R
        $es = tc.es
        delete sy&RXing
        es && esy&TXing and sy&RXing = esy&TXing
        
        # A for the such time
        c.ov = 333
        c.pre_Pieringi = &re{
            $be = me&fiu,E,e.t,8
            if (be) {
                ec&b = be
                me&tc,'b','b'
            }
            # apply new E/**
            # < only staged, via ^^^Piing
            # R%E/e
            me&fiu,E,e,1
            
            # e/ref/ is staging to PE/ref/ (in &Eref_begin)
            #  transact also unsorted list of innards,
            #   which may export what we will be looking for
            $Pr = me.cbu(r,'^^^Piing')
            $pend = Prs&Epending ||= []
            pend.push(e)
            ac(r,'commit',&{
                grepout(pend,e)
