                $aq = fi&TXi,e||E,name,{t:k},c
                
                ex(aq.sc,g)
        # < most matches want to apply only to E/*
        #   applying further is wander, ie E/**
        #   then there's one-per:
        #    putting the RX.ignore on the matcher
                ah(aq,'also','dl',1)
                # < use
                init and init(aq)
                # then more inside
                z and sol(z,aq)
            }
        }
        return &h{ sol(h) }
    }
    
  // R'The' stylemo
    # 3 realises R/r as E/e
   // &Pieri, +r, &Pieringi
