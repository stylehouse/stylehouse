    # post-measure
    me.Topping_41 = &acgtRN{
        # (light here-ish chases away %indim)
        $len = Rs&geolen
        if (Rs&isdim) {
            # are always containers of sketchables
            if (len < 9 && !hak(N)) {
                # contains nothing
                # < make sure...? %dim 
