        # non-%%arg, esp as Go is a row
        #  brack only allows other rows as %%n
        $Go
        $x
        su('Reside',{perm:&zaa,jaa,ha,ar{
            
            x = me&Gome_Jx,R
            !x and debugger
            s != xs&Gome and me&tc,"~Gome"
            
          # the entire @Gome 
            Go = theone(me&jaa,R,'Go' )
            !Go and return ha('pur:!Go')
