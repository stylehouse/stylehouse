            me&Eref_begin,r,E
        }
        if (c.Top) {
            rc&N = []
            rc&d = 0
            rc&dl = 15
            rc&nl = 300
            delete rc&refR
            delete rc&refs
            # lets /Display** recycle Elements
            me&arriveRbitX,r,r.t
            # new J, normal mind
            rs&task ||= 'Pi_The'
        }
        if (E) {
            tax(r.c,E.c,'nl,dl')
            Ec&rc_mix and ex(r.c,Ec&rc_mix)
        }
        # 13 puts on tape or not
        # < splice in re ry&up, neighbours
        rc&N.push(r)
    }
    
  // Pi%Pier, &Ring thence
    me.Pi_Pier = &acgt{ return [
       // %Top|%Pier back
        ['Pier',0.2,&acgtRs{
