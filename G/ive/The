            if (rs&Msink) {
                # new since specialfx
                $late = rs&M.slice(since)
                $lat = grepout(rs&M,late)
                hak(lat) != hak(late) and debugger
                # scooped into a row
                lat.map(n => ac(rs&Msink,'z',n))
            }
        }
        $M = Rs&M
        vsortz(M)
    }
    # upwards, cook modulat
    me.specialfx1 = &acgtR{
        $C = Rs&C
